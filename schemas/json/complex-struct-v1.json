{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "$metadata": {
    "uris": {
      "jsonLdContext": "http://47.242.107.228:3003/iden3/claim-schema-vocab/main/schemas/json-ld/complex-struct-v1.jsonld",
      "jsonSchema": "http://47.242.107.228:3003/iden3/claim-schema-vocab/main/schemas/json/complex-struct-v1.json"
    }
  },
  "required": [
    "@context",
    "id",
    "type",
    "issuanceDate",
    "credentialSubject",
    "credentialSchema",
    "credentialStatus",
    "issuer"
  ],
  "properties": {
    "@context": {
      "type": [
        "string",
        "array",
        "object"
      ]
    },
    "id": {
      "type": "string"
    },
    "type": {
      "type": [
        "string",
        "array"
      ],
      "items": {
        "type": "string"
      }
    },
    "issuer": {
      "type": [
        "string",
        "object"
      ],
      "format": "uri",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        }
      }
    },
    "issuanceDate": {
      "type": "string",
      "format": "date-time"
    },
    "expirationDate": {
      "type": "string",
      "format": "date-time"
    },
    "credentialSchema": {
      "type": "object",
      "required": [
        "id",
        "type"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        },
        "type": {
          "type": "string"
        }
      }
    },
    "subjectPosition": {
      "type": "string",
      "enum": [
        "none",
        "index",
        "value"
      ]
    },
    "merklizationRootPosition": {
      "type": "string",
      "enum": [
        "none",
        "index",
        "value"
      ]
    },
    "revNonce": {
      "type": "integer"
    },
    "version": {
      "type": "integer"
    },
    "updatable": {
      "type": "boolean"
    },
    "credentialSubject": {
      "type": "object",
      "required": [
        "id",
        "passportInfo"
      ],
      "properties": {
        "id": {
          "title": "Credential Subject ID",
          "type": "string",
          "format": "uri"
        },
        "passportInfo": {
          "type": "object",
          "properties": {
            "birthday": {
              "type": "integer"
            }
          },
          "required": [
            "birthday"
          ]
        }
      }
    }
  }
}
